{
  "name": "@anglish/pipeline",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "type": "module",
  "version": "0.1.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "etl": "pnpm run fetch && pnpm run parse && pnpm run normalize-pre && pnpm run merge && pnpm run normalize-post && pnpm run map",
    "fetch": "tsx src/cli.ts fetch",
    "fetch:force": "tsx src/cli.ts fetch --force",
    "parse": "tsx src/cli.ts parse",
    "normalize-pre": "tsx src/cli.ts normalize-pre",
    "merge": "tsx src/cli.ts merge",
    "normalize-post": "tsx src/cli.ts normalize-post",
    "map": "tsx src/cli.ts map"
  },
  "devDependencies": {
    "@anglish/config": "workspace:*",
    "@anglish/core": "workspace:*",
    "@anglish/db": "workspace:*",
    "cheerio": "1.1.2",
    "colors": "1.4.0",
    "node-fetch": "^3.3.2",
    "openai": "5.23.2",
    "pg": "8.16.3",
    "redis": "^5.10.0",
    "tsx": "^4.21.0",
    "turndown": "^7.2.2",
    "yaml": "^2.8.2"
  },
  "dependencies": {
    "kysely": "0.28.9"
  }
}
