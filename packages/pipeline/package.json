{
  "name": "@anglish/pipeline",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "type": "module",
  "version": "0.1.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "etl": "pnpm run fetch && pnpm run parse && pnpm run normalize && pnpm run merge && pnpm run map",
    "fetch": "tsx src/cli.ts fetch",
    "fetch:force": "tsx src/cli.ts fetch --force",
    "parse": "tsx src/cli.ts parse",
    "normalize": "tsx src/cli.ts normalize",
    "merge": "tsx src/cli.ts merge",
    "map": "tsx src/cli.ts map",
    "wordnet:fetch": "dotenv -e ../../.env -- tsx src/wordnet/cli.ts fetch",
    "wordnet:embed": "dotenv -e ../../.env -- tsx src/wordnet/cli.ts embed",
    "wordnet:load": "dotenv -e ../../.env -- tsx src/wordnet/cli.ts load",
    "wordnet:index": "dotenv -e ../../.env -- tsx src/wordnet/cli.ts index"
  },
  "devDependencies": {
    "@anglish/config": "workspace:*",
    "@anglish/core": "workspace:*",
    "@anglish/db": "workspace:*",
    "cheerio": "1.1.2",
    "colors": "1.4.0",
    "openai": "5.23.2",
    "pg": "8.16.3",
    "redis": "^5.10.0",
    "yaml": "^2.8.2"
  }
}
